{
  "expo": {
    "name": "Tracks",
    "slug": "Tracks",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "be.webmindz.tracks",
      "supportsTablet": true
    },
    "android": {
      "package": "be.webmindz.tracks",
      "versionCode": 1
    },
    "description": "This is my first React Native application based on the online Udemy Course (https://www.udemy.com/course/the-complete-react-native-and-redux-course/)."
  }
}
